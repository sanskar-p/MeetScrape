{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"],
        "persistent": true 
      },
      "browser_action": {},
    "content_scripts": [
      {
        "matches": [
          "https://meet.google.com/*", 
          "http://meet.google.com/*"
        ],
        "js": ["content.js"]
      }
    ]
  }